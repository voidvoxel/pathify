{
  "name": "pathify",
  "version": "0.1.6",
  "description": "Path manipulation and resolution library",
  "main": "src/index.mjs",
  "type": "module",
  "scripts": {
    "build": "npm run build-types",
    "build-types": "tsc src/**.mjs --declaration --allowJs --emitDeclarationOnly --outDir types",
    "test": "node --experimental-vm-modules node_modules/jest/bin/jest.js",
    "prepublish": "npm run build"
  },
  "keywords": [
    "path",
    "manipulation",
    "manipulate",
    "join",
    "concat",
    "concatenate",
    "replace",
    "extension",
    "ext",
    "file",
    "directory",
    "name",
    "filename"
  ],
  "author": "Ashlynne Juniper",
  "license": "MPL-2.0",
  "dependencies": {
    "pathabs": "github:voidvoxel/pathabs#v1.0.0",
    "replace-end": "github:voidvoxel/replace-end"
  },
  "devDependencies": {
    "jest": "^29.7.0",
    "typescript": "^5.4.5"
  },
  "types": "types/index.d.mts",
  "engines": {
    "node": "^20.0.0"
  }
}
